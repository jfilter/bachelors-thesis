journal = {Data and Knowledge Engineering},
keywords = {Algorithms,Closure systems,Database analysis,Formal concept analysis,Knowledge discovery,Lattices},
mendeley-groups = {07\_FourthResearch,08\_FifthReseach},
number = {2},
